                        <div class="SeparatorFull"/>
                        <div class="Container100 Responsive50">
                            <div class="EmptyBox10"/>
                            <h:graphicImage name="climaFamiliar1.jpg" library="images" style="width:150px; height:150px;"/>    
                        </div>
                            <f:metadata>
                                <f:event type="preRenderView" listener="#{iniciaTcfsoControler.init()}"/>
                            </f:metadata>
                        <div class="EmptyBox20"/>
                        <center>
                            <p:commandButton value="Iniciar Test" action="#{testCfsoControler.init()}" styleClass="CyanButton" icon="fa fa-star Fs16 White" style="width: auto;" rendered="#{iniciaTcfsoControler.boton}"/>    
